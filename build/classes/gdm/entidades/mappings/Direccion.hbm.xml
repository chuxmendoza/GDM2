<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="gdm.entidades.clases.Direccion" table="direccion">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>        
        <property name="calle" type="string" column="calle" length="80"/>
        <property name="numero" type="string" column="numero" length="80"/>
        <property name="colonia" type="string" column="colonia" length="80"/>
        <property name="ciudad" type="string" column="ciudad" length="80"/>     
        
    </class>
</hibernate-mapping>