<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="gdm.entidades.clases.Directorio" table="directorios">
    <id name="id" type="java.lang.Integer">
      <column name="id"/>
      <generator class="identity"/>
    </id>
 
  <property name="contratos" type="string" column="contratos" length="500"/>
  <property name="recibos" type="string" column="recibos" length="500"/>
  <property name="modelos" type="string" column="modelos" length="500"/>
  <property name="agradecimientos" type="string" column="agradecimientos" length="500"/> 
  </class>
</hibernate-mapping>
