<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="gdm.entidades.clases.FotoPanoramica" table="fotospanoramicas">
    <id name="id" type="java.lang.Integer">
      <column name="id"/>
      <generator class="identity"/>
    </id>
 
  <property name="fecha" type="date"/> 
  <property name="lugar" type="string" column="lugar" length="300"/>
  <property name="hora" type="int" column="hora"/>
  <property name="minutos" type="int" column="minutos"/>
   </class>
</hibernate-mapping>
